{
    "manifest_version": 3,
    "name": "Main Controller",
    "version": "1.0",
    "description": "Sends commands to play YouTube videos on client devices.",
    "permissions": ["tabs"],
    "host_permissions": [
        
            "https://remote-control-7c5u.onrender.com/",
            "wss://remote-control-7c5u.onrender.com/"
            
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
